<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:skin="http://schemas.android.com/android/skin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        layout="@layout/leftback_toobar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></include>


    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/activity_changeskin_rc"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"></com.jcodecraeer.xrecyclerview.XRecyclerView>

    <Button
        android:id="@+id/btn_from_not"
        style="@style/btnStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@color/color_66bc5a"
        android:padding="5dp"
        android:text="经典红"
        skin:enable="true" />

    <Button
        android:id="@+id/btn_from_net"
        style="@style/btnStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@color/item_bg"
        android:padding="5dp"
        android:text="屌丝灰"
        skin:enable="true" />


</LinearLayout>